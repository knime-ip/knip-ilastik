<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.knime.knip.base.prefs.MainPreferencePage"
            class="org.knime.knip.ilastik.nodes.IlastikPreferencePage"
            id="org.knime.knip.ilastik.nodes.IlastikPreferencePage"
            name="Ilastik">
      </page>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
      		category-path="/community/knip/ilastik"
            factory-class="org.knime.knip.ilastik.nodes.headless.IlastikHeadlessNodeFactory"
            id="org.knime.knip.ilastik.nodes.headless.IlastikHeadlessNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/ilastik-icon.ico"
            level-id="ilastik"
            locked="true"
            name="Ilastik"
            path="/community/knip">
      </category>
   </extension>
</plugin>
